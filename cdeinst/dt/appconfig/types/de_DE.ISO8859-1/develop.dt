# ##################################################################
# 
# Datenattribute
# 
# ##################################################################
DATA_ATTRIBUTES CORE
{
        ACTIONS         (None)
        ICON		Dtcore
        MIME_TYPE	application/octet-stream
        SUNV3_TYPE      corefile
	DESCRIPTION	Diese Datei enthält ein Kernabbild für einen Prozeß, \
                        der abnormal beendet wurde. Der Datentyp dieser Datei ist \
                        CORE.
        TYPE_LABEL      CORE
}
DATA_CRITERIA CORE1
{
	DATA_ATTRIBUTES_NAME CORE
        MODE            f
	NAME_PATTERN	core
}
DATA_CRITERIA CORE2
{
	DATA_ATTRIBUTES_NAME CORE
        MODE            f
	NAME_PATTERN	core*
}
ACTION Print
{
        LABEL           Drucken
        ARG_TYPE        CORE
        TYPE            MAP
        MAP_ACTION      NoPrint
}

