ACTION OpenFolder
{
        LABEL           	Abrir
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		Edit
        TT_FILE		   	%Arg_1"Carpeta a abrir:"%
