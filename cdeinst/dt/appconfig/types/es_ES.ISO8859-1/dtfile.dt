ACTION DtfileHome
{
        LABEL           	Gestor de archivos - Inicio
        ARG_COUNT		0
        ICON			Fphome
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		DtHome_Show
        DESCRIPTION     	La acción Gestor de archivos - Inicio (DtfileHome) abre \
                        una vista del Gestor de archivos de la carpeta \
                        de inicio del usuario.
}

# #####

ACTION Dtfile
{
        LABEL           	Gestor de archivos
        ICON			Fphome
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		Edit
        TT_FILE		   	%Arg_1"Carpeta a abrir:"%
        TT_ARG0_MODE		TT_INOUT
        TT_ARG0_VTYPE   	FILE_NAME
        DESCRIPTION     	La acción Gestor de archivos (Dtfile) abre una vista del \
                        Gestor de archivos de la carpeta especificada.
}

