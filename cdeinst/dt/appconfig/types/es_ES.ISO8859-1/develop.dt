# ###########################################################

DATA_ATTRIBUTES MAKEFILE
{
        ACTIONS         Open,Make,Print
        ICON		DtbigM
        IS_TEXT		true
        NAME_TEMPLATE	Makefile
        MIME_TYPE	text/plain
        SUNV3_TYPE      Makefile
	DESCRIPTION	Es un archivo Makefile. Un archivo Makefile describe \
                        cómo generar un archivo ejecutable a partir de un \
                        conjunto de archivos fuente. Lo utiliza el mandato \
                        make (hacer). Su tipo de datos se denomina MAKEFILE. Los \
                        archivos MAKEFILE deben llamarse Makefile o makefile \
                        o sus nombres deben empezar por Makefile o makefile \
                        o finalizar en '.mk'.
        TYPE_LABEL      MAKEFILE
}
DATA_CRITERIA MAKEFILE1
{
	DATA_ATTRIBUTES_NAME MAKEFILE
        MODE            f
        NAME_PATTERN    [Mm]akefile*
}
DATA_CRITERIA MAKEFILE2
{
	DATA_ATTRIBUTES_NAME MAKEFILE
        MODE            f
        NAME_PATTERN    *.mk
}
ACTION Make
{
        LABEL           Hacer
        TYPE            COMMAND
        ARG_TYPE        MAKEFILE
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING     sh -c '             make -f "%(File)Arg_1%" %"Destino para Hacer:"% 2>&1 | \${PAGER:-more};             echo "\\n*** Seleccione Cerrar o Salir en el menú de la ventana para cerrar esta ventana ***"'


        DESCRIPTION     La acción Hacer para archivos Makefiles solicita \
                        el destino y después utiliza el mandato 'make' de \
                        Unix para hacerlo. Si el destino se deja vacío \
                        el mandato hace 'todo' de la carpeta actual.
}


# ###########################################################

DATA_ATTRIBUTES IMAKEFILE
{
        ACTIONS         Open,Make,Print
        ICON            Dtimake
        IS_TEXT         true
        NAME_TEMPLATE	Imakefile
        MIME_TYPE	text/plain
        DESCRIPTION     Es un archivo Imakefile. Un archivo Imakefile describe \
                        cómo generar un archivo ejecutable a partir de \
                        un conjunto de archivos fuente. Lo utiliza el mandato \
                        make (hacer). Su tipo de datos se denomina IMAKEFILE. Los \
                        archivos IMAKEFILE se llaman imakefile o Imakefile \
                        o tienen nombres que empiezan por imakefile o Imakefile.
        TYPE_LABEL      IMAKEFILE
}
DATA_CRITERIA IMAKEFILE1
{
        DATA_ATTRIBUTES_NAME IMAKEFILE
        MODE            f
        NAME_PATTERN    [Ii]makefile*
}
ACTION Make
{
        LABEL           Hacer
        TYPE            COMMAND
        ARG_TYPE        IMAKEFILE
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING     sh -c '             make Makefile 2>&1 | \${PAGER:-more};             echo "\\n*** Seleccione Cerrar o Salir en el menú de la ventana para cerrar esta ventana ***"'


        DESCRIPTION     El mandato Hacer para archivos Imakefile utiliza el \
                        mandato 'make' para crear el archivo Makefile asociado.
}

# ##########################################################################
# ##########################################################################
# 
# Acciones
# 
# ##########################################################################

ACTION Nm
{
        LABEL           Lista de símbolos
        TYPE            COMMAND
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING     sh -c '                  nm "%(File)Arg_1"Nombre del archivo:"%" 2>&1 | \${PAGER:-more};                  echo "\\n*** Seleccione Cerrar o Salir en el menú de la ventana para cerrar esta ventana ***"'


        DESCRIPTION     La acción Lista de símbolos (Nm) lista la tabla \
                        de símbolos de un archivo de objetos o de un archivo \
                        de bibliotecas. Utiliza el mandato 'nm'.
}
ACTION PrintNm
{
        LABEL           Imprimir lista de símbolos
        TYPE            COMMAND
        WINDOW_TYPE     NO_STDIO
        EXEC_STRING     /bin/sh -c ' nm "%(File)Arg_1%" |                         /usr/dt/bin/dtlp -u "Lista de símbolos"'

        DESCRIPTION     La acción Imprimir lista de símbolos (PrintNm) imprime \
                        la salida del mandato 'nm' en la impresora predeterminada.
}


ACTION Make
{
        LABEL           Hacer
        TYPE            COMMAND
        ARG_COUNT       0
        WINDOW_TYPE     PERM_TERMINAL
        EXEC_STRING               sh -c '             make -f "%(File)Arg_1"Makefile:"%" %"Destino para Hacer:"% 2>&1 |               \${PAGER:-more};             echo "\\n*** Seleccione Cerrar o Salir en el menú de la ventana para cerrar esta ventana ***"'




        DESCRIPTION     La acción Hacer sin argumentos solicita un archivo \
                        Makefile y el destino y después utiliza el mandato \
                        'make' de Unix para hacerlo.
}
