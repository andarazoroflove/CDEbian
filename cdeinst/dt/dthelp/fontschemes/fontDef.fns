*.p.12.roman.bold.sans_serif.*.ISO-8859-1: \
		-dt-application-bold-r-normal-sans-*-140-*-*-p-*-iso8859-1"
*.p.14.roman.bold.sans_serif.*.ISO-8859-1: \
