#include "Dt"

Dticon*fileMenu.exit.acceleratorText:          Alt+F4
Dticon*fileMenu.exit.accelerator:              Alt<Key>F4
Dticon*editMenu.undo.acceleratorText:          Alt+Retour arrière
Dticon*editMenu.undo.accelerator:              Alt<Key>BackSpace:
Dticon*editMenu.cut.acceleratorText:           Maj+Suppr
Dticon*editMenu.cut.accelerator:               Shift<Key>DeleteChar:
Dticon*editMenu.copy.acceleratorText:          Ctrl+Inser
Dticon*editMenu.copy.accelerator:              Ctrl<Key>InsertChar:
Dticon*editMenu.paste.acceleratorText:         Maj+Inser
Dticon*editMenu.paste.accelerator:             Shift<Key>InsertChar:

Dticon*fileMenu.new.acceleratorText:           Ctrl+N
Dticon*fileMenu.new.accelerator:               Ctrl<Key>N
Dticon*fileMenu.open.acceleratorText:          Ctrl+O
Dticon*fileMenu.open.accelerator:              Ctrl<Key>O
Dticon*fileMenu.save.acceleratorText:          Ctrl+S
Dticon*fileMenu.save.accelerator:              Ctrl<Key>S
Dticon*fileMenu.saveas.acceleratorText:        Ctrl+A
Dticon*fileMenu.saveas.accelerator:            Ctrl<Key>A

Dticon*instructionText.alignment:              alignment_beginning

Dticon*useMessaging:                           True
Dticon*useFileFilter:                          False
!#Dticon*secondaryColorSetId:                    1

