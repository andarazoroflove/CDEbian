.TH "Dt/Help\&.h" "file formats"
.SH "NAME"
\fBDt/Help\&.h\fP \(em help services definitions
.SH "SYNOPSIS"
.PP
.nf
#include <Dt/Help\&.h>
.fi
.SH "DESCRIPTION"
.PP
